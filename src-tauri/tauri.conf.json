{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "schachterl",
	"version": "0.1.0",
	"identifier": "dev.fnnln.schachterl",
	"build": {
		"frontendDist": "../build",
		"devUrl": "http://localhost:5174",
		"beforeDevCommand": "npm run dev:tauri",
		"beforeBuildCommand": "npm run build:tauri"
	},
	"app": {
		"windows": [
			{
				"title": "schachterl",
				"width": 800,
				"height": 600,
				"maximized": true,
				"resizable": true,
				"fullscreen": false,
				"decorations": true,
				"devtools": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
